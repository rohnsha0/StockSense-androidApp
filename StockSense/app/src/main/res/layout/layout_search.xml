<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="45dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/setIndexH1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirm Index"
        android:textSize="19sp"
        android:fontFamily="@font/titilliumweb_semi_bold"
        android:layout_marginTop="29dp"
        android:layout_marginStart="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/searchIndex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:textSize="17sp"
        android:fontFamily="@font/titilliumweb_semi_bold"
        android:layout_marginEnd="30dp"
        android:textColor="@color/dash_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/setIndexH1"
        app:layout_constraintBottom_toBottomOf="@id/setIndexH1"/>

</androidx.constraintlayout.widget.ConstraintLayout>